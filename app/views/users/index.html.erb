<div class="jumbotron">
        <h1 class="text-center">All Students</h1>
    </div>
    
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Full Name</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
      <% @user.each do |u| %>
        <tr>
          <td><a href="users/<%= u.id %>"><%= u.name %></a></td>
          <td><%= u.email %></td>
        </tr>
      <% end %>
      </tbody>
    </table> 